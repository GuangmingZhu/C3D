train_net: "chalearn_isogr_depth_train.prototxt"
base_lr: 0.0005
momentum: 0.9
weight_decay: 0.005
lr_policy: "step"
gamma: 0.1
stepsize: 15000
# Display every 200 iterations
display: 20
# The maximum number of iterations
max_iter: 45000
# snapshot intermediate results
snapshot: 1000
snapshot_prefix: "chalearn_isogr_depth"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 0
